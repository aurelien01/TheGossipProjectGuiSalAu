<h1>Connexion</h1>

<div class="container col-md-8 order-md-1">
<%= form_tag url_for(action: 'create'), method: 'post' %>
<div class="form-group">
<%= label_tag 'E-mail :' %>
<%= text_field_tag 'email',"", class: "form-control"  %><br>

<%= label_tag 'Mot de passe :' %>
<%= password_field_tag 'password',"", class: "form-control"  %><br>

<%= submit_tag 'Connexion', class: "btn btn-primary" %>

</div>
</div>
